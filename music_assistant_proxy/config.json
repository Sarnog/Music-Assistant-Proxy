{
  "name": "Music Assistant Proxy",
  "version": "1.0.1",
  "slug": "music_assistant_proxy",
  "description": "Proxy voor Music Assistant met Web Interface",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "hassio_api": true,
  "hassio_role": "default",
  "webui": "http://[HOST]:[PORT:8095]",
  "ingress": true,
  "ingress_port": 8095,
  "panel_icon": "/music_assistant_proxy/icon/music_assistant.ico",
  "panel_title": "Music Assistant",
  "host_network": true,
  "ports": {
    "8095/tcp": 8095
  },
  "ports_description": {
    "8095/tcp": "Music Assistant Web Interface"
  },
  "options": {
    "server_host": "127.0.0.1",
    "server_port": 8095
  },
  "schema": {
    "server_host": "str",
    "server_port": "port"
  },
  "startup": "application",
  "boot": "auto",
  "stage": "stable"
}
