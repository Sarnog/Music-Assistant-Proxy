{
  "name": "Music Assistant Proxy",
  "version": "1.0.0",
  "slug": "music_assistant_proxy",
  "description": "Proxy voor Music Assistant met Web Interface",
  "url": "https://github.com/Sarnog/Music-Assistant-Proxy",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "init": true,
  "stage": "stable",
  "startup": "services",
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "webui": "http://[HOST]:[PORT:8095]",
  "ingress": true,
  "ingress_port": 8095,
  "panel_icon": "mdi:music",
  "panel_title": "Music Assistant",
  "ports": {
    "8095/tcp": 8095
  },
  "ports_description": {
    "8095/tcp": "Music Assistant Web Interface"
  },
  "options": {
    "server_ip": "",
    "server_port": 8095
  },
  "schema": {
    "server_ip": "str",
    "server_port": "int"
  },
  "image": "ghcr.io/sarnog/{arch}-addon-music_assistant_proxy"
}
